.box1{
	border:1px solid #C9C9C9;
	width: 250px;
	margin-top: 10px;
	float: left;
}

.box1 h4{
	background-color: #EEEEEE;
	padding:5px 0px 5px 20px;
	font-weight: normal; 
}

.box1 li::before{
    background-image: url("../images/icon_total.png");
    content: "";
    display: block;
    float: left;
    height: 12px;
    margin: 11px 6px 0 0;
    width: 12px;
}
.box1 li{    
	border-bottom: 1px solid #d1d1d1;
    cursor: pointer;
    font-size: 12px;
    line-height: 36px;
    margin: 0 auto;
    width: 230px;				
}
.box1 li:last-child{
	border-bottom:0px;
}

.email-left-mk1 li::before{
    background-position: -253px -123px;
}

.email-left-mk2 li::before{
    background-position: -320px -124px;
}

