@import url('css.css');
@import url('frame.css');
@import url('Pop.css');
/* copy */
.Content { width: 1000px; margin: 0 auto; padding-bottom:40px; color: #31333e}
.Content-left-three- { width: 213px; height: 229px; border: 1px solid #c9c9c9; background-color: white; margin-top: 13px; }
.Content-left-three-img- { text-align: center; }

/* Content-Left */

.Content-Left { width: 215px; float: left; }
.Content-head { width: 1000px; height: 45px; font-family: 微软雅黑; }
.Content-head-logo { background-image: url(../images/icon_total.png); width: 29px; height: 27px; background-position: -335px -93px; float: left; margin-top: 10px; }
.Content-head-navts { margin-left: 15px; float: left; font-size: 14px; line-height: 26px; margin-top: 10px; }
.Content-head-navwz { font-size: 14px; line-height: 26px; cursor: pointer; margin-top: 10px; float: left; }

.Hbsj_C_T_C_Left{ width: 215px; height: 100%; float: left; border: solid 1px #0096dc;}
.Hbsj_C_T_C_L_Top{ width: 100%; height: 35px; background-color: #0096dc; color: white; text-align: center; line-height: 35px; font-size: 14px;}
.Hbsj_C_T_C_L_Bottom{ width: 100%; height: 240px; background-color: #eeeeee;}
.Hbsj_C_T_C_L_B_Div{ border-bottom: solid 1px #d1d1d1; border-top:solid 1px white; width: 90%; height: 35px; margin: auto;}
.Hbsj_C_T_C_L_B_D_Left{ width: 15px; height: 15px; background-image: url(../images/icon_total.png); background-position: -204px -121px; margin-top: 10px; float: left;}
.Hbsj_C_T_C_L_B_D_Text{ font-size: 12px; color: #31333e; float: left; line-height: 35px; margin-left: 5px;}
.Hbsj_C_T_C_L_B_Div:hover{ background:#fff; margin:0; padding:0 11px; margin:0; position:relative;}
.Hbsj_C_T_C_L_B_Div:hover .Hbsj_C_T_C_L_B_D_Text{ color:#0096dc; margin-left:0;}
.Hbsj_C_T_C_L_B_Div:hover .Hbsj_C_T_C_L_B_D_Left{ background:url(../images/hoverIcon.png) 0 0 no-repeat; margin-top:12px; margin-left:1px; margin-right:3.5px;}

.Hbsj_C_T_C_L_B_Div:hover .Hbsj_C_T_C_L_B_D_Box{ display:block;}
.Hbsj_C_T_C_L_B_D_Box{ display:none; position:absolute; left:215px; background:white; height:16px; padding:10px 10px; margin-top:-2px; width:264px; border:1px solid #0096dc; border-left:none; z-index:999;}
.adsaf{ font-size:12px; color:#31333e;}
.adsaf li{ float:left;}
.adsaf div{ float:left; margin:0 8px;}

/* Content-Right */
.Content_Right { width: 780px; float: right; }
.Content_Right_Banner { width: 780px; line-height: 273px; background-color: #31333e; margin:0px auto; text-align: center; font-family: 微软雅黑; font-size: 14px; color: White; position: relative; }
.Content_Right_Banner_Change { bottom: 10px; position: absolute; margin-left: 355px; }
.Change_Ul .active { background-position: -247px -147px; }
.Change_Circle { cursor: pointer;  background: url(../images/icon_total.png); width: 9px; height: 11px; float: left; margin-left: 10px; background-position: -233px -147px;  }

/* CR_HostRegStep */
.CR_HostRegStep{width:100%;padding-bottom:20px;}
.CR_HRS_Header{width:100%;line-height:36px;font-size:14px;background-color:#eee;}
.CR_HRS_HText{margin-left:14px;}
.CR_HRS_Header a{color:#0096dc;font-size:12px;text-decoration:underline;margin-right:24px;}
.CR_HRS_Content{width: 760px;margin: 20px auto;}
.CR_HRS_CIcon{position: relative;width:88px;height:88px;background-image:url(../images/step_icn.png);float:left;padding: 0px 4px;}
.CR_HRS_CIcon span{position:absolute;text-align:center;width: 100%;margin-top: 94px;}
.CIcon1{background-position: 8px 0px;}
.CIcon2{background-position: -92px 0px;}
.CIcon3{background-position: -190px 0px;}
.CIcon4{background-position: -286px 0;}
.CIcon5{background-position: -382px 0;}
.CR_HRS_Accos{width:29px;height:25px;background-image:url(../images/step_icn.png);background-position: -492px -32px;margin: 30px 18px;float: left;}

/* CR_HostSearch */

.CR_HostSearch{width:740px;margin:0px auto; line-height: 60px;}
.CR_HostSearch .CR_EnterBox{padding-left: 190px;font-size: 20px;line-height:54px;}
.CR_HostSearch .CR_EnterBox .host{width:216px;line-height:26px;background-color:#fff;border:1px solid #cacaca;margin:0px 8px;font-size: 14px;}
.CR_HostSearch .CR_EnterBox .btn1{width:88px;line-height:26px;}
.CR_HostSearch .CR_CheckBox{border: 1px solid #febc0e;background-color: #fffaec;line-height: 40px;padding-left: 20px;}
.CR_HostSearch .CR_CheckBox label {width: 100px;float: left;}
.CR_CheckBox label .hot{width: 21px;height: 10px;background-image: url(../images/icon_total.png);background-position: -227px -123px;float: right;margin: 12px 40px 0px 0px;}

.CR_HostValue{width:100%;margin:0px auto;border:1px solid #c9c9c9;}
.CR_HostValue .title{line-height: 36px; background-color: #eee; color: #31333e; font-size: 14px; font-family: 微软雅黑; padding-left:14px;}
.CR_HostValue ul{padding-left:14px;}
.CR_HostValue ul li{line-height: 40px;width: 150px;margin-right: 130px;}
.CR_HostValue ul .nomr{margin-right:0;}
.CR_HostValue ul li:nth-child(3n){margin-right:0;}
